{
  "name": "@KazuhikoKuroda/jsvardump-gpr",
  "version": "1.0.3",
  "description": "Output javascript object like PHP(var_dump) using console.log.",
  "main": "jsvardump.js",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "uglify-js": "^2.5.0"
  },
  "scripts": {
    "test": "npm run jshint && npm run mochatest && npm run coverage",
    "jshint": "jshint jsvardump.js",
    "mochatest": "mocha test.js",
    "coverage": "istanbul cover -x 'test.js' _mocha test.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "build": "uglifyjs jsvardump.js -c \"evaluate=false\" --comments \"/    .*/\" -m --source-map jsvardump-min.map -o jsvardump-min.js"
  },
  "files": [
    "jsvardump.js",
    "jsvardump-min.js",
    "jsvardump-min.map",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "javascript",
    "dump"
  ],
  "author": "kurom96",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://KazuhikoKuroda@github.com/KazuhikoKuroda/jsvardump.git"
  },
  "bugs": {
    "url": "https://github.com/KazuhikoKuroda/jsvardump/issues"
  },
  "homepage": "https://github.com/KazuhikoKuroda/jsvardump",
  "publishConfig": { "registry": "https://npm.pkg.github.com/" }
}
